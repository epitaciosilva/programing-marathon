40 40 50 60 10 80
(80, 90, 10, 70 90)

40 40 10 10 80
(80,50,20,90)

40 40 20 80
(80,60,0)

40 40 0
(80,0)

------------
40 40 50
(80,90) -> (80,20)

40 40 50 60
(80,90,10) -> (80,50)

40 40 50 60 10
(80,90,10,70) -> (80,50,20)

40 40 20 60 40 90 | m = []
40+40 || 40 + 50 -> m+= [80,50]	
80 + 50 || 50+60 -> m+=[10]
10+10 || 90+10 -> m+=[0]







